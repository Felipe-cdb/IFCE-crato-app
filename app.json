{
  "expo": {
    "name": "IFCE Crato",
    "slug": "ifce-crato-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pedro_almeida.ifcecratoapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "303e911f-5936-40e7-a352-81465a680a1d"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir que o aplicativo tenha acesso aos seus arquivos de imagem?",
          "cameraPermission": "Permitir que o aplicativo tenha acesso a sua c√¢mera?"
        }
      ]
    ]
  }
}
