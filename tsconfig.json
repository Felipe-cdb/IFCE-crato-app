{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "..",
    "strict": true,
    "moduleResolution": "node",
    // "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck":true,
    "paths": {
      "@env": ["node_modules/react-native-dotenv"]
    }
  }
}
